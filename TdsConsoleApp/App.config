<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <connectionStrings>
    <clear />
    <add name="DatabaseConnection"
     providerName="System.Data.ProviderName"
     connectionString="Data Source = JARVIS; Initial Catalog = TestBase; Integrated Security = SSPI;" />
  </connectionStrings>
  <appSettings>
    <add key="DatabaseLogPath" value="D:\TDS\SQLlog.txt"/>
    <add key="XmlStructuresPath" value="D:TDS\Structures"/>
    <add key="SaveEntityPath" value ="D:TDS\SavedEntities"/>
  </appSettings>
</configuration>