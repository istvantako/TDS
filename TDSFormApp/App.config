<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <log4net>
    <appender name="MsSqlRfa" type="log4net.Appender.RollingFileAppender">
      <file value="D:\\TDS\\MsSqlLog.txt" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <countDirection value="-1" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="MsSqlRfa" />
    </root>
  </log4net>
  <connectionStrings>
    <clear />
    <add name="DatabaseConnection"
     providerName="System.Data.ProviderName"
     connectionString="Data Source = Lajos-PC; Initial Catalog = Switches; Integrated Security = SSPI;" />
  </connectionStrings>
  <appSettings>
    <add key = "TdsStoragePath" value = "D:\\TDS" />
    <add key = "EntityManagerLogPath" value = "D:\\TDS\\EntityManagerLog.txt" />
  </appSettings>
</configuration>